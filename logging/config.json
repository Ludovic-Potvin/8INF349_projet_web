{
    "version": 1,
    "formatters": {
      "default": { "format": "[%(asctime)s] %(levelname)s in %(module)s: %(message)s" }
    },
    "handlers": {
        "stdout": {
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout",
            "formatter": "default"
        },
        "log_file": {
            "class": "logging.FileHandler",
            "filename": "app.log",
            "mode": "a",
            "formatter": "default"
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": ["stdout", "log_file"]
    }
}